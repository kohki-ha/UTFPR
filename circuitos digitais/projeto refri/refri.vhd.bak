library IEEE;
use IEEE.std_logic_1164.all;
use IEEE.numeric_std.all;

entity refri is
        port(
            clock     :   in std_logic;
            enable    :   in std_logic;    -- decodificador entrada de seleção
            botton1   :   in std_logic;    -- botao para confirmar escolha do produto
            botton2   :   in std_logic;    -- botao para parar de escolher produtos
            switch	  :   in std_logic_vector (9 downto 0);    -- escolher os produtos
            display1  :  out std_logic_vector ( 6 downto 0)    -- display de 7 segmentos
        );
end refri;